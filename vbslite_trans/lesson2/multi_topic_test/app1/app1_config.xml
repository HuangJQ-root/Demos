<?xml version="1.0" ?>
<dds>
	<idl>HelloWorld.idl</idl>
	<network>
		<master>
			<!-- 替换成本地ip -->
			<address>127.0.0.1</address>
			<netmask>255.255.255.0</netmask>
		</master>
	</network>
	<discovery>
		<announce>
			<times>50</times>
			<period>3 0</period>
		</announce>
		<heartbeat>
			<per_samples>5</per_samples>
			<period>5 0</period>
		</heartbeat>
		<liveliness>
			<lease_duration>10 0</lease_duration>
			<period>5 0</period>
		</liveliness>
	</discovery>
	<participant>
		<name>node_1</name>
		<domain_id>0</domain_id>
		<max_remote_participants>9</max_remote_participants>
		<writer>
			<key>w1</key>
			<topic>MVBS_topic1</topic>
			<type>MVBS::HelloWorld</type>
			<qos>default</qos>
			<max_remote_peers>2</max_remote_peers>
		</writer>
		<writer>
			<key>w2</key>
			<topic>MVBS_topic2</topic>
			<type>MVBS::BasicTypeStruct</type>
			<qos>default</qos>
			<max_remote_peers>2</max_remote_peers>
		</writer>
		<writer>
			<key>w3</key>
			<topic>MVBS_topic3</topic>
			<type>MVBS::sdatatype</type>
			<qos>default</qos>
			<max_remote_peers>2</max_remote_peers>
		</writer>
		<writer>
			<key>w4</key>
			<topic>MVBS_topic4</topic>
			<type>MVBS::ComplexDataType</type>
			<qos>default</qos>
			<max_remote_peers>2</max_remote_peers>
		</writer>
		<writer>
			<key>w5</key>
			<topic>MVBS_topic5</topic>
			<type>StructSize50</type>
			<qos>default</qos>
			<max_remote_peers>2</max_remote_peers>
		</writer>
		<reader>
			<key>r1</key>
			<topic>MVBS_topic6</topic>
			<type>MVBS::HelloWorld1</type>
			<qos>default</qos>
			<max_remote_peers>2</max_remote_peers>
		</reader>
		<reader>
			<key>r2</key>
			<topic>MVBS_topic7</topic>
			<type>MVBS::HelloWorld2</type>
			<qos>default</qos>
			<max_remote_peers>2</max_remote_peers>
		</reader>
		<reader>
			<key>r3</key>
			<topic>MVBS_topic8</topic>
			<type>MVBS::HelloWorld3</type>
			<qos>default</qos>
			<max_remote_peers>2</max_remote_peers>
		</reader>
		<reader>
			<key>r4</key>
			<topic>MVBS_topic9</topic>
			<type>MVBS::HelloWorld4</type>
			<qos>default</qos>
			<max_remote_peers>2</max_remote_peers>
		</reader>
		<reader>
			<key>r5</key>
			<topic>MVBS_topic10</topic>
			<type>MVBS::HelloWorld5</type>
			<qos>default</qos>
			<max_remote_peers>2</max_remote_peers>
		</reader>
	</participant>
</dds>
